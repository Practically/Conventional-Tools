{
    "name": "@baln/conventional-tools",
    "description": "CLI tool for a conventional workflow",
    "version": "0.4.0",
    "author": {
        "name": "Ade Attwood ",
        "email": "code@adeattwood.co.uk",
        "url": "http://adeattwood.co.uk"
    },
    "bin": {
        "conventional-tools": "./bin/run"
    },
    "dependencies": {
        "@commitlint/config-conventional": "^12",
        "@commitlint/format": "^12",
        "@commitlint/lint": "^12",
        "@commitlint/load": "^12",
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-autocomplete": "^0.3.0",
        "@oclif/plugin-help": "^3",
        "chalk": "^4.0.0",
        "conventional-changelog": "^3.1.18",
        "conventional-recommended-bump": "^6.0.5",
        "cosmiconfig": "^7.0.0",
        "execa": "^5.0.0",
        "form-data": "^4.0.0",
        "got": "^11.8.2",
        "listr": "^0.14.3",
        "tempfile": "^3.0.0",
        "tslib": "^2"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "@types/chai": "^4",
        "@types/conventional-changelog": "^3.1.0",
        "@types/conventional-recommended-bump": "^6.0.0",
        "@types/git-semver-tags": "^4.0.0",
        "@types/listr": "^0.14.2",
        "@types/mocha": "^8",
        "@types/node": "^15",
        "@types/semver": "^7.1.0",
        "chai": "^4",
        "globby": "^11",
        "mocha": "^8",
        "nyc": "^15",
        "prettier": "^2.0.5",
        "sinon": "^11.1.0",
        "ts-node": "^10",
        "typescript": "^4"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://conventional-tools.practically.io/",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "conventional-tools",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete"
        ]
    },
    "repository": "https://git.zportal.co.uk/practically-oss/conventional-tools",
    "scripts": {
        "build": "oclif-dev pack",
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
        "test": "TS_NODE_PROJECT=\"test/tsconfig.json\" nyc --extension .ts mocha --timeout 30000 --async-only --recursive --reporter spec --require ts-node/register --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "lint": "prettier --write ./src/**/*.{js,jsx,ts,tsx,json,md,css} ./docs/**/*.{js,jsx,ts,tsx,json,md,css}"
    },
    "types": "lib/index.d.ts"
}
